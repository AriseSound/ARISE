OUTFD=/proc/self/fd/$2;
ui_print() { echo -e "ui_print $1\nui_print" > $OUTFD; }

safe_mount() {
	IS_MOUNTED=$(cat /proc/mounts | grep "$1")
	if [ "$IS_MOUNTED" ]; then
		ui_print "$1 mounted, remounting as rw"
		mount -o remount,rw $1
	else
		ui_print "$1 not mounted, mounting as rw"
		mount $1
	fi

}
safe_mount /system
safe_mount /data
safe_mount /vendor

SYS=/system

if [ -d "/system/system" ]; then
  ui_print "Device found as Pixel. Using /system/system instead of /system"
	SYS=/system/system
fi

set_perm() {
 if [ -e $1 ]; then
  chown $2:$3 $1 || exit 1
  chmod $4 $1 || exit 1
  if [ "$5" ]; then
  chcon $5 $1 2>/dev/null
  else
  chcon 'u:object_r:system_file:s0' $1 2>/dev/null
  fi
 fi
}

set_perm_recursive() {
  find $1 -type d 2>/dev/null | while read dir; do
    set_perm $dir $2 $3 $4 $6
  done
  find $1 -type f 2>/dev/null | while read file; do
    set_perm $file $2 $3 $5 $6
  done
}

set_metadata() {
  file="$1";
  if [ -e $file ]; then
    shift;
    until [ ! "$2" ]; do
    case $1 in
      uid) chown $2 $file;;
      gid) chown :$2 $file;;
      mode) chmod $2 $file;;
      capabilities) ;;
      selabel)
      chcon -h $2 $file;
      chcon $2 $file;
      ;;
      *) ;;
    esac;
    shift 2;
    done;
  fi
}

set_metadata_recursive() {
  dir="$1";
  shift;
  until [ ! "$2" ]; do
    case $1 in
      uid) chown -R $2 $dir;;
      gid) chown -R :$2 $dir;;
      dmode) find "$dir" -type d -exec chmod $2 {} +;;
      fmode) find "$dir" -type f -exec chmod $2 {} +;;
      capabilities) ;;
      selabel)
      fir
        find "$dir" -exec chcon -h $2 '{}' +;
        find "$dir" -exec chcon $2 '{}' +;
      ;;
      *) ;;
    esac;
    shift 2;
  done;
}

getprop() { test -e /sbin/getprop && /sbin/getprop $1 || grep "^$1" /default.prop; }

