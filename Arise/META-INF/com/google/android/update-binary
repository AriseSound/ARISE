    cp $CLIB86/libv4a_fx_jb_X86.so $BOUTIQUE/$V4ALIB
  else
    cp $CARM/$V4AUTIL $LIB/$V4AUTIL
    cp $CARM/libv4a_fx_jb_NEON.so $BOUTIQUE/$V4ALIB
  fi
